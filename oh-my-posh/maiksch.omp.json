{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#45F1C2",
          "style": "plain",
          "template": "(\uF0E7)",
          "type": "root"
        },
        {
          "foreground": "#FFFFFF",
          "template": "",
          "type": "text",
          "style": "plain"
        },
        {
          "foreground": "#0FD3FF",
          "properties": {
            "folder_separator_icon": "/",
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#FF9248",
          "style": "plain",
          "template": " \ue725:{{ .HEAD }}{{ .BranchStatus }}",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "branch_icon": ""
          },
          "type": "git"
        },
        {
          "foreground": "#33DD2D",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}#f1184c{{ end }}"],
          "template": " \u276f ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
